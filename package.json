{
  "name": "flashcards",
  "version": "0.1.0",
  "devDependencies": {
    "react-scripts": "0.8.5",
    "sw-precache": "^4.3.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
